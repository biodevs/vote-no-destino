<div class="row">
  <div class="col s12 m12 l8 offset-l2">
      <div class="card-panel">
        <div class="card-content">
          <div class="card-title">
            Destinos mais votados
            <span class="right">total: <span class="badge strong"><%=@votes.values.sum%>votos</span></span>
          </div>
        </div>
      </div>
    <% @votes.each do |id, total|%>
      <%=destiny(id, total)%>
    <%end%>
  </div>
</div>