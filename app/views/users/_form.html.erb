<%=form_for(@user) do |form|%>
  <div class="row">
    <div class="col s6 offset-s3">
      <%=render partial: 'shared/error_messages', locals: {object: @user}  %>
      <div class="card">
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4">Finalizar Voto</span>
          <div class="input-field">
            <%=form.text_field :name, class: 'validate'%>
            <%=form.label :name%>
          </div>
          <div class="input-field">
            <%=form.email_field :email, class: 'validate' %>
            <%=form.label :email, data: {error:"formato invÃ¡lido"} %>
          </div>
          <div class="input-field">
            <%=form.submit "Confirmar", class:"btn waves-effect waves-light" %>
          </div>
        </div>
      </div>
    </div>
  </div>
<%end%>